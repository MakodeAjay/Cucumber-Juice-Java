<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Selenium4 Cucumber Hybrid Framework Suite" data-provider-thread-count="2">
    <test name="Test On Chrome" preserve-order="true" >
        <parameter name="cucumber.filter.tags" value="@regression"></parameter>
        <parameter name="browserName" value="chrome"></parameter>
        <classes>
            <class name="cucumberOptions.TestNGRunner"/>
        </classes>
    </test>

    <test name="Test Basics" preserve-order="true" >
        <parameter name="cucumber.filter.tags" value="@regression"></parameter>
        <parameter name="browserName" value="safari"></parameter>
        <classes>
            <class name="cucumberOptions.TestNGRunner"/>
        </classes>
    </test>
</suite>